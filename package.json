{
  "name": "ts-bicycle",
  "version": "4.0.1",
  "description": "Type safe version of bicycle",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "bin": "./lib/cli.js",
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "bicycle": "*"
  },
  "dependencies": {
    "@types/mkdirp": "^0.5.0",
    "@types/node": "^7.0.31",
    "lsr": "^2.0.0",
    "mkdirp": "^0.5.1",
    "prettier": "^1.6.0",
    "stable-stringify": "^1.0.0",
    "tsconfig": "^6.0.0",
    "typescript": "^2.4.1"
  },
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "bicycle": "^8.0.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.1",
    "opaque-types": "^1.1.2",
    "ts-jest": "^20.0.7"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier:check": "prettier --list-different \"src/**/*.ts\"",
    "build": "opaque-types src && tsc",
    "test": "jest ./src --coverage",
    "watch": "jest ./src --coverage --watch"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*.test.(ts|tsx|js)"
    ]
  }
}